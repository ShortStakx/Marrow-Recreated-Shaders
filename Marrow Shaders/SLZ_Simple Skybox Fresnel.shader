				float3 normalizeResult91 = normalize( WorldViewDirection );
				float3 break43 = ( (  ( -normalizeResult91 - 0.0 > 0.0 ? ( _Boxscale1 + _Pos1 ) : -normalizeResult91 - 0.0 <= 0.0 && -normalizeResult91 + 0.0 >= 0.0 ? 0.0 : ( -_Boxscale1 + _Pos3 ) )  - WorldPosition ) / -normalizeResult91 );
				float2 uv_BumpMap1 = IN.ase_texcoord8.xyz.xy * _BumpMap1_ST.xy + _BumpMap1_ST.zw;
				float3 unpack83 = UnpackNormalScale( tex2D( _BumpMap1, uv_BumpMap1 ), _NormalScale1 );
				unpack83.z = lerp( 1, unpack83.z, saturate(_NormalScale1) );
				float3 tanToWorld0 = float3( WorldTangent.x, WorldBiTangent.x, WorldNormal.x );
				float3 tanToWorld1 = float3( WorldTangent.y, WorldBiTangent.y, WorldNormal.y );
				float3 tanToWorld2 = float3( WorldTangent.z, WorldBiTangent.z, WorldNormal.z );
				float3 ase_tanViewDir =  tanToWorld0 * WorldViewDirection.x + tanToWorld1 * WorldViewDirection.y  + tanToWorld2 * WorldViewDirection.z;
				ase_tanViewDir = normalize(ase_tanViewDir);
				float3 normalizeResult82 = normalize( ase_tanViewDir );
				float dotResult84 = dot( unpack83 , normalizeResult82 );
				float4 texCUBENode64 = texCUBElod( _BaseMap, float4( ( ( -normalizeResult91 * min( min( break43.x , break43.y ) , break43.z ) ) + ( WorldPosition - _Pos1 ) ), ( ( ( 1.0 - dotResult84 ) * _Power ) * 5.0 )) );
				float4 temp_output_62_0 = ( texCUBENode64 * _EmissionColor * dotResult84 );
				
